generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model short_urls {
  id         Int      @id @default(autoincrement())
  code       String   @unique(map: "idx_short_urls_code")
  target_url String
  created_at DateTime @default(now()) @db.Timestamptz(6)
}
